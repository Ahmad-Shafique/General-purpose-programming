class Hello{
private int var1;
private String var2;
private float var3;


Hello()
{
this(0,"Empty",0);
}

Hello(int var)
{
this(var,"Empty",0);
}

Hello(int var, String s1 )
{
this(var,s1,0);
}

Hello(String s1)
{
this(0,s1,0);
}

Hello(int var , String s1 , float f)
{
var1 = var;
var2 = s1;
var3 = f;
}


void print()
{
System.out.println(var1 +" " + var2 + " " + var3);
}

public static void main(String []args)
{
Hello h1=new Hello("There it is");
h1.print();
}

}
